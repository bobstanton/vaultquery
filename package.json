{
	"name": "vaultquery",
	"version": "1.0.0",
	"description": "Index notes and front matter into SQLite database for structured querying with enhanced UI and modern design",
	"main": "main.js",
	"types": "index.d.ts",
	"scripts": {
		"rename": "node -e \"require('fs').rename('main.css', 'styles.css', () => { })\"",
		"generate-help": "node ../md-to-obsidian-dom/dist/bin/generate.js ./help-md ./src/generated-help --prefix vaultquery-help",
		"dev": "npm run generate-help && node esbuild.config.mjs && npm run rename",
		"build": "npm run generate-help && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && npm run rename",
		"version": "node version-bump.mjs && git add manifest.json versions.json && npm run rename"
	},
	"keywords": [],
	"author": "Bob Stanton",
	"license": "MIT",
	"devDependencies": {
		"@codemirror/language": "6.10.8",
		"@codemirror/state": "6.5.0",
		"@codemirror/view": "6.38.6",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.39.1",
		"@types/fnv-plus": "^1.3.2",
		"@types/node": "^25.0.2",
		"@types/sql.js": "^1.4.9",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"builtin-modules": "5.0.0",
		"esbuild": "^0.27.1",
		"eslint": "^9.39.1",
		"eslint-plugin-obsidian-extras": "file:../eslint-plugin-obsidian-extras",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"obsidian": "latest",
		"tslib": "2.8.1",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"chart.js": "^4.5.0",
		"fnv-plus": "^1.3.1",
		"markdown-table": "^3.0.4",
		"placeholder-resolver": "file:../placeholder-resolver",
		"slickgrid": "^5.15.5",
		"sql.js": "^1.13.0"
	}
}
